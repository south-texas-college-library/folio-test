{
    "displayName": "Circulation - Loan Items",
    "description": "Retrieving loan items",
    "instructions": "Input an item barcode, item status, and date range for results",
    "parameters": [
        {
            "name": "item_barcode",
            "displayName": "Item Barcode",
            "type": "text",
            "required": false
        },
        {
            "name": "item_status",
            "displayName": "Item Status",
            "type": "text",
            "required": true,
            "default": "All",
            "controlled.options": [
                "All",
                "Aged to lost",
                "Available",
                "Checked out",
                "In transit",
                "Missing",
                "Unavailable",
                "Unknown",
                "Withdrawn"
            ]
        },
        {
            "name": "start_date",
            "displayName": "Start Date",
            "type": "date",
            "required": false
        },
        {
            "name": "end_date",
            "displayName": "End Date",
            "type": "date",
            "required": false
        }
    ]
}